# Only track large theme packages with LFS
*.theme.zip filter=lfs diff=lfs merge=lfs -text size:+75M

# Don't use LFS for smaller files
*.bg.zip -filter -diff -merge -text
*.icon.zip -filter -diff -merge -text
*.over.zip -filter -diff -merge -text
*.bg.zip !text !filter !merge !diff
*.icon.zip !text !filter !merge !diff
*.over.zip !text !filter !merge !diff
small_files.txt !text !filter !merge !diff
* !text !filter !merge !diff
Packages/themes/Game-Rentals.theme.zip filter=lfs diff=lfs merge=lfs -text
Upload/Game-Rentals.theme.zip filter=lfs diff=lfs merge=lfs -text
